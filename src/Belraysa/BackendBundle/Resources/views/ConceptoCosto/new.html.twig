<form action="{{ path('conceptocosto_create') }}" role="form" id="newConceptoCosto" method="post" {{ form_enctype(form) }}
      class="form-horizontal">
    <div class="form-group">
        <label class="col-sm-2 control-label"><span style="color: red"
                                                    class="required">*&nbsp;</span>Nombre</label>

        <div class="col-sm-10">
            {{ form_widget(form.nombre, {'attr' : {'class' : 'form-control','placeholder':'Nombre' }}) }}
            {{ form_errors(form.nombre) }}
        </div>
    </div>
    {{ form_rest(form) }}
</form>
