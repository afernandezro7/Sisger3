<form action="{{ path('service_create1') }}" role="form" id="newService" method="post" {{ form_enctype(form) }}
      class="form-horizontal">
    <div class="form-group">
        <label class="col-sm-2 control-label"><span style="color: red"
                                                    class="required">*&nbsp;</span>Nombre</label>

        <div class="col-sm-10">
            {{ form_widget(form.name, {'attr' : {'class' : 'form-control','placeholder':'Nombre' }}) }}
            {{ form_errors(form.name) }}
        </div>
    </div>

    {{ form_rest(form) }}
</form>
