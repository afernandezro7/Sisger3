{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Gasto</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Detalles</th>
                <td>{{ entity.detalles }}</td>
            </tr>
            <tr>
                <th>Paguea</th>
                <td>{{ entity.pagueA }}</td>
